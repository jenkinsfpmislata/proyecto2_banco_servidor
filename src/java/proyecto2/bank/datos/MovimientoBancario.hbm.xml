<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.MovimientoBancario" table="movimientobancario" >
        <id column="idMovimientoBancario" name="idMovimientoBancario" type="integer"/>
        
        <property name="tipoMovimientoBancario" >
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.fpmislata.banco.TipoMovimientoBancario</param>
                <param name="type">12</param>
            </type>
        </property>
        
        <property name="importe" />
        <property name="fecha" />
        <property name="saldoTotal" />
        <property name="concepto" />
      
    </class>
</hibernate-mapping>